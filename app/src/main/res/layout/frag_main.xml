<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="frag_main"
            type="String" />
    </data>

    <RelativeLayout
        android:id="@+id/rl_sec"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <EditText
            android:id="@+id/editText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="212dp"
            android:layout_marginTop="67dp"
            android:ems="10"
            android:inputType="textPassword" />

        <SeekBar
            android:id="@+id/seekBar"
            style="@style/Widget.AppCompat.SeekBar.Discrete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/editText"
            android:layout_below="@+id/editText"
            android:layout_marginTop="61dp"
            android:max="10"
            android:progress="3" />

        <ProgressBar
            android:id="@+id/progressBar"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/seekBar"
            android:layout_below="@+id/seekBar"
            android:layout_marginEnd="19dp"
            android:layout_marginTop="67dp" />

        <CheckedTextView
            android:id="@+id/checkedTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/editText"
            android:layout_marginEnd="34dp"
            android:layout_marginTop="18dp"
            android:text="CheckedTextView" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/progressBar"
            android:layout_below="@+id/progressBar"
            android:layout_marginTop="41dp"
            android:text="Button" />

        <Spinner
            android:id="@+id/spinner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/button"
            android:layout_alignStart="@+id/progressBar" />

        <EditText
            android:id="@+id/editText2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/button"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="49dp"
            android:ems="10"
            android:inputType="textPassword" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/seekBar"
            android:layout_below="@+id/editText2"
            app:srcCompat="@drawable/bg_btn_nor" />

        <ImageSwitcher
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/seekBar"
            android:layout_marginTop="18dp"
            android:layout_toStartOf="@+id/editText2"
            android:id="@+id/imageSwitcher" />

        <com.android.lib.view.photoview.AppPhotoView
            android:id="@+id/appPhotoView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/imageSwitcher"
            android:layout_below="@+id/editText"
            android:layout_marginEnd="25dp"
            app:srcCompat="@drawable/actionbar_shadow_up" />

        <com.android.lib.view.other.AppViewPager
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/editText2"
            android:layout_alignEnd="@+id/editText2"
            android:layout_marginEnd="16dp"
            android:id="@+id/appViewPager" />

        <com.android.lib.view.viewpager.MyViewPager
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignEnd="@+id/imageButton"
            android:layout_below="@+id/imageButton"
            android:layout_marginTop="35dp"
            android:id="@+id/myViewPager" />

        <ViewStub
            android:id="@+id/viewStub"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/checkedTextView"
            android:layout_alignTop="@+id/imageSwitcher"
            android:layout="@layout/activity_image_preview" />

        <com.android.lib.view.photoview.AppPhotoView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/checkedTextView"
            android:layout_below="@+id/appViewPager"
            android:layout_marginTop="13dp"
            app:srcCompat="@drawable/bg_cricle_yel" />

        <com.android.lib.view.photoview.AppPhotoView
            android:layout_width="wrap_content"
